{"ast":null,"code":"import PostForm from \"@/components/PostForm.vue\";\nimport PostList from \"@/components/PostList.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    PostForm,\n    PostList\n  },\n\n  data() {\n    return {\n      posts: [],\n      dialogVisible: false,\n      isPostsLoading: false,\n      selectedSort: ''\n    };\n  },\n\n  methods: {\n    createPost(post) {\n      this.posts.push(post);\n      this.dialogVisible = false;\n    },\n\n    removePost(post) {\n      this.posts = this.posts.filter(p => p.id !== post.id);\n    },\n\n    showDialog() {\n      this.dialogVisible = true;\n    },\n\n    async fetchPosts() {\n      try {\n        this.isPostsLoading = true;\n        const response = await axios.get(\"https://jsonplaceholder.typicode.com/posts?_limit=10\"); // получаем данные с сервера\n\n        this.posts = response.data;\n        console.log(response);\n      } catch (e) {\n        alert('Error');\n      } finally {\n        this.isPostsLoading = false;\n      }\n    },\n\n    selectedSort() {}\n\n  },\n\n  mounted() {\n    this.fetchPosts();\n  }\n\n};","map":{"version":3,"mappings":"AAoCA,OAAOA,QAAP,MAAqB,2BAArB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,UAAU,EAAE;IACVH,QADU;IAEVC;EAFU,CADC;;EAMbG,IAAI,GAAG;IACL,OAAM;MACJC,KAAK,EAAE,EADH;MAEJC,aAAa,EAAE,KAFX;MAGJC,cAAc,EAAE,KAHZ;MAIJC,YAAY,EAAE;IAJV,CAAN;EAMD,CAbY;;EAebC,OAAO,EAAE;IACPC,UAAU,CAACC,IAAD,EAAO;MACf,KAAKN,KAAL,CAAWO,IAAX,CAAgBD,IAAhB;MACA,KAAKL,aAAL,GAAqB,KAArB;IACD,CAJM;;IAKPO,UAAU,CAACF,IAAD,EAAO;MACf,KAAKN,KAAL,GAAa,KAAKA,KAAL,CAAWS,MAAX,CAAkBC,KAAKA,CAAC,CAACC,EAAF,KAASL,IAAI,CAACK,EAArC,CAAb;IACD,CAPM;;IAQPC,UAAU,GAAG;MACX,KAAKX,aAAL,GAAqB,IAArB;IACD,CAVM;;IAWP,MAAMY,UAAN,GAAmB;MACjB,IAAI;QACF,KAAKX,cAAL,GAAsB,IAAtB;QACA,MAAMY,QAAO,GAAI,MAAMjB,KAAK,CAACkB,GAAN,CAAU,sDAAV,CAAvB,CAFE,CAEuF;;QACzF,KAAKf,KAAL,GAAac,QAAQ,CAACf,IAAtB;QACAiB,OAAO,CAACC,GAAR,CAAYH,QAAZ;MACF,CALA,CAKE,OAAOI,CAAP,EAAU;QACVC,KAAK,CAAC,OAAD,