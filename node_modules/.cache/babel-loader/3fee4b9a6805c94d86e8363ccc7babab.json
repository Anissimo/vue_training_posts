{"ast":null,"code":"import PostForm from \"@/components/PostForm.vue\";\nimport PostList from \"@/components/PostList.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    PostForm,\n    PostList\n  },\n\n  data() {\n    return {\n      posts: [{\n        id: 1,\n        title: 'JS0',\n        body: \"description of the post 0\"\n      }, {\n        id: 2,\n        title: 'JS1',\n        body: \"description of the post 1\"\n      }, {\n        id: 3,\n        title: 'JS2',\n        body: \"description of the post 2\"\n      }],\n      dialogVisible: false\n    };\n  },\n\n  methods: {\n    createPost(post) {\n      this.posts.push(post);\n      this.dialogVisible = false;\n    },\n\n    removePost(post) {\n      this.posts = this.posts.filter(p => p.id !== post.id);\n    },\n\n    showDialog() {\n      this.dialogVisible = true;\n    },\n\n    async fetchPosts() {\n      try {\n        const response = await axios.get(\"https://jsonplaceholder.typicode.com/posts?_limit=10\"); // получаем данные с сервера\n\n        console.log(response);\n      } catch (e) {\n        alert('Error');\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,QAAP,MAAqB,2BAArB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,UAAU,EAAE;IACVH,QADU;IAEVC;EAFU,CADC;;EAMbG,IAAI,GAAG;IACL,OAAM;MACJC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAN;QAASC,KAAK,EAAE,KAAhB;QAAuBC,IAAI,EAAE;MAA7B,CADK,EAEL;QAAEF,EAAE,EAAE,CAAN;QAASC,KAAK,EAAE,KAAhB;QAAuBC,IAAI,EAAE;MAA7B,CAFK,EAGL;QAAEF,EAAE,EAAE,CAAN;QAASC,KAAK,EAAE,KAAhB;QAAuBC,IAAI,EAAE;MAA7B,CAHK,CADH;MAMJC,aAAa,EAAE;IANX,CAAN;EAQD,CAfY;;EAiBbC,OAAO,EAAE;IACPC,UAAU,CAACC,IAAD,EAAO;MACf,KAAKP,KAAL,CAAWQ,IAAX,CAAgBD,IAAhB;MACA,KAAKH,aAAL,GAAqB,KAArB;IACD,CAJM;;IAKPK,UAAU,CAACF,IAAD,EAAO;MACf,KAAKP,KAAL,GAAa,KAAKA,KAAL,CAAWU,MAAX,CAAkBC,KAAKA,CAAC,CAACV,EAAF,KAASM,IAAI,CAACN,EAArC,CAAb;IACD,CAPM;;IAQPW,UAAU,GAAG;MACX,KAAKR,aAAL,GAAqB,IAArB;IACD,CAVM;;IAWP,MAAMS,UAAN,GAAmB;MACjB,IAAI;QACF,MAAMC,QAAO,