{"ast":null,"code":"import PostForm from \"@/components/PostForm.vue\";\nimport PostList from \"@/components/PostList.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    PostForm,\n    PostList\n  },\n\n  data() {\n    return {\n      posts: [],\n      dialogVisible: false\n    };\n  },\n\n  methods: {\n    createPost(post) {\n      this.posts.push(post);\n      this.dialogVisible = false;\n    },\n\n    removePost(post) {\n      this.posts = this.posts.filter(p => p.id !== post.id);\n    },\n\n    showDialog() {\n      this.dialogVisible = true;\n    },\n\n    async fetchPosts() {\n      try {\n        const response = await axios.get(\"https://jsonplaceholder.typicode.com/posts?_limit=10\"); // получаем данные с сервера\n\n        this.posts = response.data;\n        console.log(response);\n      } catch (e) {\n        alert('Error');\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,QAAP,MAAqB,2BAArB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,UAAU,EAAE;IACVH,QADU;IAEVC;EAFU,CADC;;EAMbG,IAAI,GAAG;IACL,OAAM;MACJC,KAAK,EAAE,EADH;MAEJC,aAAa,EAAE;IAFX,CAAN;EAID,CAXY;;EAabC,OAAO,EAAE;IACPC,UAAU,CAACC,IAAD,EAAO;MACf,KAAKJ,KAAL,CAAWK,IAAX,CAAgBD,IAAhB;MACA,KAAKH,aAAL,GAAqB,KAArB;IACD,CAJM;;IAKPK,UAAU,CAACF,IAAD,EAAO;MACf,KAAKJ,KAAL,GAAa,KAAKA,KAAL,CAAWO,MAAX,CAAkBC,KAAKA,CAAC,CAACC,EAAF,KAASL,IAAI,CAACK,EAArC,CAAb;IACD,CAPM;;IAQPC,UAAU,GAAG;MACX,KAAKT,aAAL,GAAqB,IAArB;IACD,CAVM;;IAWP,MAAMU,UAAN,GAAmB;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAN,CAAU,sDAAV,CAAvB,CADE,CACuF;;QACzF,KAAKb,KAAL,GAAaY,QAAQ,CAACb,IAAtB;QACAe,OAAO,CAACC,GAAR,CAAYH,QAAZ;MACF,CAJA,CAIE,OAAOI,CAAP,EAAU;QACVC,KAAK,CAAC,OAAD,CAAL;MACF;IACF;;EAnBO;AAbI,CAAf","names":["PostForm","PostList","axios","components","data","posts","dialogVisible","methods","createPost","post","push","removePost","filter","p","id","showDialog","fetchPosts","response","get","console","log","e","alert"],"sourceRoot":"","sources":["C:\\Users\\Annni\\Desktop\\myPurpose\\training\\project1\\src\\App.vue"],"sourcesContent":["<template>\n  <div class = \"app\">\n    <h3>Page with posts</h3>\n    \n    <my-button\n    @click=\"showDialog\"\n    style=\"margin: 15px 0\"\n    >Сreate a user</my-button>\n    \n    <my-button\n    @click=\"fetchPosts\"\n    >\n      Get posts\n    </my-button>\n    \n    <my-dialog v-model:show=\"dialogVisible\">\n      <post-form\n      @create=\"createPost\"\n      />\n    </my-dialog>\n\n    <post-list\n      :posts=\"posts\"\n      @remove=\"removePost\"\n    />\n  </div>\n</template>\n\n\n<script>\nimport PostForm from \"@/components/PostForm.vue\";\nimport PostList from \"@/components/PostList.vue\";\nimport axios from \"axios\";\n \nexport default {\n  components: {\n    PostForm,\n    PostList,\n  },\n\n  data() {\n    return{\n      posts: [],\n      dialogVisible: false, \n    }\n  },\n\n  methods: {\n    createPost(post) {\n      this.posts.push(post);\n      this.dialogVisible = false;\n    },\n    removePost(post) {\n      this.posts = this.posts.filter(p => p.id !== post.id)\n    },\n    showDialog() {\n      this.dialogVisible = true;\n    },\n    async fetchPosts() {\n      try {\n        const response = await axios.get(\"https://jsonplaceholder.typicode.com/posts?_limit=10\") // получаем данные с сервера\n        this.posts = response.data;\n        console.log(response);\n      } catch (e) {\n        alert('Error')\n      }\n    }        \n  },\n}\n</script>\n<!-- \n  https://jsonplaceholder.typicode.com/posts?_limit=10 \n\n  fetch('https://jsonplaceholder.typicode.com/todos/1')\n  .then(response => response.json())\n  .then(json => console.log(json))\n-->\n\n<style>\n* {\n  color: #616161;\n  background-color: #212121;\n  padding: 0;\n  box-sizing: border-box;\n}\n.app{\n  padding: 20px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}