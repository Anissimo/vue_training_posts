{"ast":null,"code":"import PostForm from \"@/components/PostForm.vue\";\nimport PostList from \"@/components/PostList.vue\";\nexport default {\n  components: {\n    PostForm,\n    PostList\n  },\n\n  // дефолтные переменные\n  data() {\n    return {\n      posts: [{\n        id: 1,\n        title: 'JS0',\n        body: \"description of the post 0\"\n      }, {\n        id: 2,\n        title: 'JS1',\n        body: \"description of the post 1\"\n      }, {\n        id: 3,\n        title: 'JS2',\n        body: \"description of the post 2\"\n      }],\n      title: \"\",\n      body: \"\"\n    };\n  },\n\n  // методы крч наши\n  methods: {\n    createPost() {\n      const newPost = {\n        id: Date.now(),\n        title: this.title,\n        body: this.body\n      };\n      this.posts.push(newPost);\n      this.title = \"\";\n      this.body = \"\";\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoCA,OAAOA,QAAP,MAAqB,2BAArB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AAEA,eAAe;EACbC,UAAU,EAAE;IACVF,QADU;IACAC;EADA,CADC;;EAIb;EACAE,IAAI,GAAG;IACL,OAAM;MACJC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAN;QAASC,KAAK,EAAE,KAAhB;QAAuBC,IAAI,EAAE;MAA7B,CADK,EAEL;QAAEF,EAAE,EAAE,CAAN;QAASC,KAAK,EAAE,KAAhB;QAAuBC,IAAI,EAAE;MAA7B,CAFK,EAGL;QAAEF,EAAE,EAAE,CAAN;QAASC,KAAK,EAAE,KAAhB;QAAuBC,IAAI,EAAE;MAA7B,CAHK,CADH;MAMJD,KAAK,EAAE,EANH;MAOJC,IAAI,EAAE;IAPF,CAAN;EAUD,CAhBY;;EAkBb;EACAC,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,MAAMC,OAAM,GAAI;QACdL,EAAE,EAAEM,IAAI,CAACC,GAAL,EADU;QAEdN,KAAK,EAAE,KAAKA,KAFE;QAGdC,IAAI,EAAE,KAAKA;MAHG,CAAhB;MAKA,KAAKH,KAAL,CAAWS,IAAX,CAAgBH,OAAhB;MACA,KAAKJ,KAAL,GAAa,EAAb;MACA,KAAKC,IAAL,GAAY,EAAZ;IACD;;EAVM;AAnBI,CAAf","names":["PostForm","PostList","components","data","posts","id","title","body","methods","createPost","newPost","Date","now","push"],"sourceRoot":"","sources":["C:\\Users\\Annni\\Desktop\\myPurpose\\training\\project1\\src\\App.vue"],"sourcesContent":["<template>\n  <div>\n    <div>\n      <form @submit.prevent>\n\n        <h4>Creating a post</h4>\n\n        <inp